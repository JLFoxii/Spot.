{
  "name": "db-prisma",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/db-prisma/src",
  "projectType": "library",
  "targets": {
    "prisma-generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma generate --schema=libs/db-prisma/prisma/schema.prisma"
      }
    },
    "prisma-migrate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate dev --schema=libs/db-prisma/prisma/schema.prisma"
      }
    },
    "prisma-studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma studio --schema=libs/db-prisma/prisma/schema.prisma"
      }
    },
    "seed": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx ts-node --project libs/db-prisma/tsconfig.json libs/db-prisma/prisma/seed.ts"
      }
    }
  },
  "tags": ["type:data-access"]
}
